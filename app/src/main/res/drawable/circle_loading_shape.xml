<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="48dp"
        android:left="48dp"
        android:right="48dp"
        android:top="48dp" >


        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="ring"
            android:thickness="2dp"
            android:useLevel="false">
            <size android:height="48dp" android:width="48dp"/>
            <gradient
                android:startColor="?attr/colorOutline"
                android:centerColor="?attr/colorOutline"
                android:endColor="?attr/backgroundColor"
                android:angle="220" />
        </shape>
</item>

    </layer-list>
